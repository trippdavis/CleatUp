<!DOCTYPE html>
<html>
<head>
  <title>CleatUp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  <%= current_user.username %>
  <%= button_to "Log out", session_url, method: :delete %>
<% end %>
<hr>

<%= yield %>

</body>
</html>
